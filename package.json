{
  "name": "vite-project",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc --noEmit",
    "github-build": "vite build",
    "build": "npm run generate-all && vite build",
    "preview": "vite preview",
    "submodule": "git submodule update --remote",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate-aethercurrent": "node scripts/generate-aethercurrent.cjs",
    "generate-aetheryte": "node scripts/generate-aetheryte.cjs",
    "generate-map": "node scripts/generate-map.cjs",
    "generate-action": "node scripts/generate-action.cjs",
    "generate-status": "node scripts/generate-status.cjs",
    "generate-contentFinderCondition": "node scripts/generate-contentFinderCondition.cjs",
    "generate-all": "node scripts/generate-aethercurrent.cjs && node scripts/generate-aetheryte.cjs && node scripts/generate-map.cjs && node scripts/generate-action.cjs && node scripts/generate-status.cjs && node scripts/generate-contentFinderCondition.cjs"
  },
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@element-plus/icons-vue": "^2.3.1",
    "@johnsoncodehk/html2pug": "^1.0.0",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@sweetalert2/theme-bootstrap-4": "^5.0.17",
    "@volar/pug-language-service": "^1.0.24",
    "@volar/typescript-language-service": "^1.0.1",
    "@vue/compiler-sfc": "^3.4.31",
    "@vue/language-service": "^2.0.26",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.3.1",
    "animate.css": "^4.1.1",
    "axios": "^1.7.2",
    "clipboard": "^2.0.11",
    "element-plus": "^2.7.6",
    "github-markdown-css": "^5.6.1",
    "hover.css": "^2.3.2",
    "lz-string": "^1.5.0",
    "moment": "^2.30.1",
    "obs-websocket-js": "^5.0.5",
    "pinia": "^2.1.7",
    "sweetalert2": "^11.12.2",
    "vue": "^3.4.31",
    "vue-draggable-plus": "0.2.0-beta.2",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.4.0",
    "vue-tsc": "^1.8.27",
    "vuedraggable": "^4.1.0",
    "vxe-table": "4.6.14",
    "xe-utils": "^3.5.28"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@unocss/eslint-plugin": "^0.60.4",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "fs-extra": "^11.2.0",
    "iconv-lite": "^0.6.3",
    "lint-staged": "^15.2.7",
    "opencc-js": "^1.0.5",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^4.9.5",
    "unocss": "^0.58.9",
    "unplugin-auto-import": "^0.12.2",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.5.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-sass-dts": "^1.3.22",
    "vite-plugin-style-import": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  }
}
